{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "member-access": [true, "no-public"],
    "interface-name": [true, "never-prefix"],
    "object-literal-key-quotes": [true, "as-needed"]
  },
  "rulesDirectory": []
}